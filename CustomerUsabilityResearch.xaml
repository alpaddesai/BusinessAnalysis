        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProductManagement"
        mc:Ignorable="d"
        Title="CustomerUsabilityResearch" Height="450" Width="800">
